hold on
end
hold on
for ui=1:length(uab_new(:,1))
scatter(1*(rand(1)-0.5),uab_new(ui,1),'b')
hold on
end
xlim([-1 1])
title('Visual acuity')
set(gca, 'FontSize', 14)

ylabel('degrees of visual angle')
legend('UCR old', 'UCR new', 'UAB old', 'UAB new')
print([' VA_pilot_comparison'], '-dpng', '-r300'); %<-Save as PNG with 300 DPI


