for ui=1:length(ucr_old(1,:))
themean=[mean(ucr_old(:,1)) mean(ucr_new(:,1)) mean(uab_old(:,1)) mean(uab_new(:,1))]
figure
theerr=[std(ucr_old(:,1))/sqrt(length(ucr_old(:,1))) std(ucr_new(:,1))/sqrt(length(ucr_new(:,1))) std(uab_old(:,1))/sqrt(length(uab_old(:,1))) std(uab_new(:,1))/sqrt(length(uab_new(:,1)))];
c = bar(themean);
set(gca, 'XTickLabel',{'UCR old','UCR new','UAB old', 'UAB new'})
%xlabel('Wt% of cenospheres')
ylabel('degrees of visual angle')
ylim([0 1.1])
c.FaceColor = 'flat';
% c.CData(1,:) = [0 0 1 ];
% c.CData(2,:) = [0.32 0.32 1];
% c.CData(3,:) = [0.54 0.54 1];
% c.CData(4,:) = [0.76 0.76 1];

c.CData(1,:) = [1 0 0 ];
c.CData(3,:) = [0 0 1];
c.CData(2,:) = [0 1 0 ];
c.CData(4,:) = [0.54 0.54 0.54];
%c.CData(5,:) = [0 0 1];
set(gca, 'FontSize', 18)

%errlow=(std(AcuityAcc)/sqrt(length(AcuityAcc(:,1))));
%errhigh=(std(AcuityAcc)/sqrt(length(AcuityAcc(:,1))));
errlow=theerr;
errhigh=errlow;
hold on

er = errorbar(c.XData,themean,errlow,errhigh);  
er.Color = [0 0 0];                            
er.LineStyle = 'none';
hold on
 plot(1,ucr_old(:,1),'o','LineWidth',1,...
                       'MarkerEdgeColor','k',...
                       'MarkerFaceColor','w',...
                       'MarkerSize',7)
                    plot(2,ucr_new(:,1),'o','LineWidth',1,...
                       'MarkerEdgeColor','k',...
                       'MarkerFaceColor','w',...
                       'MarkerSize',7)
                                       plot(3,uab_old(:,1),'o','LineWidth',1,...
                       'MarkerEdgeColor','k',...
                       'MarkerFaceColor','w',...
                       'MarkerSize',7)
                                       plot(4,uab_new(:,1),'o','LineWidth',1,...
                       'MarkerEdgeColor','k',...
                       'MarkerFaceColor','w',...
                       'MarkerSize',7)
                   title( 'VA comparison')
print([' VA_pilot_comparison'], '-dpng', '-r300'); %<-Save as PNG with 300 DPI



